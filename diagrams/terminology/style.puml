@startuml style
!include ../color/color.puml
hide empty members

!define taxStandard (S,pGreen) Standard
!define taxOrg (O,pOrange) Organization
!define taxWork (W,pBlue) Work Product

skinparam{
    'https://plantuml.com/skinparam
    'https://plantuml-documentation.readthedocs.io/en/latest/formatting/all-skin-params.html
 
    ArrowColor pBlack
    ArrowFontColor pBlack

    'backgroundColor transparent Don't do this because of night mode

    ClassBackgroundColor pPaleGreen
    classFontColor pBlack
    'classFontSize 10
    'classFontName Aapex
    
    HyperlinkColor pRed

    NoteBackgroundColor pYellow
    NoteBorderColor pOrange
    NoteFontColor pBlack

    TitleFontColor pBlack
}
@enduml